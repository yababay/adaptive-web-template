{
  "name": "@yababay67/vite-vanilla-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "purge-error-html": "sed -E -i 's/.assets/assets/g' dist/error.html",
    "purge-eula-html": "sed -E -i 's/.assets/assets/g' dist/eula.html",
    "purge-index-html": "sed -E -i 's/.assets/assets/g' dist/index.html",
    "docs": "rm -rf docs && rm -rf dist && npm run build && npm run purge-error-html && npm run purge-eula-html && npm run purge-index-html && mv dist docs",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run docs && git add . && git commit -am publish && git push origin main"
  },
  "devDependencies": {
    "@yababay67/postcss-bootstrap-icons": "^0.0.2",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "postcss-icon-blender": "^1.0.0-Beta.3",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "bootstrap-icons": "^1.10.3"
  }
}
